{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "linebreak-style": 0,
        "arrow-body-style": 0,
        "jsx-a11y/label-has-for": 0,
        "jsx-a11y/anchor-is-valid": ["error", {
            "components": [ "Link" ],
            "specialLink": ["to"]
        }]
    }
}